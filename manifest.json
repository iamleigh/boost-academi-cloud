{
	"manifest_version": 2,
	"name": "Boost Academi Cloud",
	"description": "This extension corrects some styling issues of Academi Cloud",
	"version": "1.0",
	"content_scripts": [{
		"matches": ["https://americanschool.academi-cloud.net/*"],
		"css": ["main.css"]
	}],
	"page_action": {
		"default_icon": "icon.png",
		"default_title": "Boost for Academi Cloud"
	}
}